{
  "name": "@fuyoo/dragresize",
  "version": "0.1.0",
  "private": false,
  "description": "[Vue3 Component] 拖拽缩放并具有自动吸附对齐、参考线等功能",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "npm run build:lib && npm run build:dts",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name DragResize src/index.ts",
    "build:dts": "tsc src/index.ts --declaration --declarationDir typings"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-plugin-vuex": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-typescript": "^11.0.2",
    "babel-plugin-import": "^1.13.6",
    "core-js": "^3.29.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vue": "^3.2.47"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuyoo/dragresize.git"
  },
  "bugs": {
    "url": "https://github.com/fuyoo/dragresize/issues"
  },
  "homepage": "https://github.com/fuyoo/dragresize",
  "keywords": [
    "vue3",
    "component",
    "draggable",
    "resizable",
    "拖拽",
    "吸附对齐"
  ],
  "files": [
    "dist",
    "typings",
    "src"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "unpkg": "src/index.js",
  "typings": "typings/index.d.ts"
}
